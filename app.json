{
  "expo": {
    "name": "Healthcare",
    "slug": "healthcare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/doctor.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#313250"
    },
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "description": "",
    "android": {
      "package": "com.routayush1.healthcare"
    }
  }
}
